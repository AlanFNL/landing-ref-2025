{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/en",
      "destination": "/index.html"
    },
    {
      "source": "/es",
      "destination": "/index.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/es/:path*",
      "destination": "/index.html"
    }
  ]
}